#4 ASCII Logo art

#include <stdio.h>

int main() 
{  
   
   
    tit();
    logi();
    printf("\n");
    titl();
    logo();
    printf("\n");
    ti();
    top();
    bo();
    lo();
    printf("\n");
    printf("Do upvote and don't forget to comment which one you like most?");
    
    
    return 0;
}
void ti()
{
    for(int s=17;s>1;s--)
           printf(" ");
    printf("THE INSTAGRAM LOGO");
    printf("\n");
    printf("\n");
}

void top()
{
    for(int i=1;i<=2;i++)
    {
        for(int s=20;s>i;s--)
           printf(" ");
        for(int k=1;k<=i;k++)
        {
            printf("#");
        }
        for(int m=1;m<=10;m++)
           printf("#");
        for(int j=1;j<=i;j++)
        {
            printf("#");
        }
        printf("\n");
    }
}
void bo()
{
    for(int i=1;i<=5;i++)
    {
        for(int s=19;s>1;s--)
           printf(" ");
        
        for(int m=1;m<=14;m++)
        { 
              if(i==1||i==5)
              {
                  if(m<=12&&m>=3)
                   printf(" ");
                  else
                   printf("#");
              }
              else if(i==3)
              {
                  if(m==7||m==8)
                    printf(" ");
                 else if(m==3||m==12)
                 printf(" ");
                  else
                   printf("#");
              }
              else
              {
           
               if(m==3||m==12)
                 printf(" ");
               
               else
                 printf("#");
                 }
        }
           
        
        printf("\n");
    }
}
void lo()
{
    for(int i=2;i>=1;i--)
    {
        for(int s=20;s>i;s--)
           printf(" ");
        for(int k=i;k>=1;k--)
        {
            printf("#");
        }
        for(int m=1;m<=10;m++)
           printf("#");
        for(int j=i;j>=1;j--)
        {
            printf("#");
        }
        printf("\n");
    }
} 
void tit()
{
    for(int s=17;s>1;s--)
           printf(" ");
    printf("THE FACEBOOK LOGO");
    printf("\n");
    printf("\n");
}
void logi()
{
    for(int i=1;i<=7;i++)
    {
        for(int s=20;s>1;s--)
           printf(" ");
        for(int k=1;k<=13;k++)
        {
            if(i==5)
            {
                if(k>=7&&k<=12)
                  printf(" ");
                else
                  printf("#");
            }
            else if(i==2)
            {
                if(k>=10&&k<=12)
                  printf(" ");
                else
                  printf("#");
            }
            else if(i>=3&&i<=7)
            {
                if(k==9||k==10)
                  printf(" ");
                else
                  printf("#");
            }
            
            else
            printf("#");
        }
        printf("\n");
}
}
void logo()
{
    for(int i=1;i<=7;i++)
    {
        for(int s=16;s>1;s--)
           printf(" ");
        for(int k=1;k<=20;k++)
        {
            
            if(i>=2&&i<=6)
            {
                if(i==4)
                {
                    if(k>=9&&k<=14)
                   printf(" ");
                  else
                   printf("#");
                }
                else if(i==3||i==5)
              {
                if(k==11||k==10||k==12||k==9)
                 printf(" ");
                else
                 printf("#");
            }
            
                else
                {
                  if(k==10||k==9)
                   printf(" ");
                  else
                   printf("#");
                 }
            }
            else if(i==1||i==7)
            {
                if(k==1||k==20)
                {
                    printf(" ");
                }
                else
                 printf("#");
            }
            else
             printf("#");
        }
        printf("\n");
}
}
void titl()
{
    for(int s=17;s>1;s--)
           printf(" ");
    printf("THE YOUTUBE LOGO");
    printf("\n");
    printf("\n");
}
